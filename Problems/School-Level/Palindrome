

//User function Template for Java

class Solution
{
    public String is_palindrome(int n)
    {
        // Code here
        int palin=0;
        int sample=n;
        while(sample>0){
            int temp=sample%10;
            palin=palin*10+temp;
            sample/=10;
        }
        if(palin==n)  return "Yes";
        else  return  "No";
    }
}
