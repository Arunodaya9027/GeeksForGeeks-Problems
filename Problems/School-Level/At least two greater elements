

//User function Template for Java

class Solution {
    public long[] findElements( long a[], long n)
    {
        // Your code goes here
        int r=(int)n-2;
        long[] ans=new long[r];
        Arrays.sort(a);
        for(int i=0; i<r; i++){
            ans[i]=a[i];
        }
        return ans;
    }
}
