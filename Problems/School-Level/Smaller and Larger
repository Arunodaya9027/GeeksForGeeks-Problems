

//User function Template for Java

class Solve {
    int[] getMoreAndLess(int[] arr, int n, int x) {
        // code here
        int[] res=new int[]{0,0};
        for(int i=0; i<arr.length; i++){
            if(arr[i]<x)  res[0]+=1;
            else if(arr[i]>x)  res[1]+=1;
            else{
                res[0]+=1;
                res[1]+=1;
            }
        }
        return res;
    }
}
